<% if @income.errors.any? %>
  $("#income_errors").addClass('alert alert-danger').html("<%= escape_javascript(render 'error') %>").show();
<% else %>
  $("#incomes tbody").prepend("<%= escape_javascript(render partial: 'income', locals: { income: @income }) %>");
  $("#chart").html("<%=escape_javascript(pie_chart @income_category_count) %>");
  $("#myModal").modal('hide');
<% end %>