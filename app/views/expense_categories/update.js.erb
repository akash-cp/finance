<% if @expense_category.errors.any? %>
$("#expense_category_errors").html("<%= escape_javascript(render 'error') %>").show();
<% else %>
$("tr[data-id='<%=@expense_category.id%>']").replaceWith("<%= escape_javascript(render partial: 'expense_category', locals: { expense_category: @expense_category }) %>");
$("#myModal").modal('hide');
<% end %>